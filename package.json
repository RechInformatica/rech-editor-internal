{
    "name": "rech-editor-internal",
    "displayName": "rech-editor-internal",
    "description": "Package for using Rech's extensions with Visual Studio Code in Rech's intranet.",
    "version": "0.0.1",
    "publisher": "Gustavo",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/RechInformatica/rech-editor-internal.git"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "rech.editor.internal.openFontesTrunk",
                "title": "openFontesTrunk"
            },
            {
                "command": "rech.editor.internal.openScripts",
                "title": "openScripts"
            },
            {
                "command": "rech.editor.internal.openWc",
                "title": "openWc"
            },
            {
                "command": "rech.editor.internal.fonGrep",
                "title": "fonGrep"
            },
            {
                "command": "rech.editor.internal.openThis",
                "title": "openThis"
            },
            {
                "command": "rech.editor.internal.RechWindowDesigner",
                "title": "RechWindowDesigner"
            }
        ],
        "keybindings": [
            {
                "command": "rech.editor.internal.openFontesTrunk",
                "key": "ctrl+shift+o"
            },
            {
                "command": "rech.editor.internal.openScripts",
                "key": "ctrl+shift+y"
            },
            {
                "command": "rech.editor.internal.openWc",
                "key": "alt+o"
            },
            {
                "command": "rech.editor.internal.fonGrep",
                "key": "ctrl+l"
            },
            {
                "command": "rech.editor.internal.openThis",
                "key": "alt+f2"
            },
            {
                "command": "rech.editor.internal.RechWindowDesigner",
                "key": "ctrl+alt+f8"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && mocha -r ts-node/register ./src/test/**/*.test.ts"
    },
    "dependencies": {
        "rech-editor-vscode": "file:../rech-editor-vscode"
    },
    "devDependencies": {
        "@types/chai": "^4.1.6",
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "typescript": "^2.6.1",
        "ts-node": "^7.0.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
