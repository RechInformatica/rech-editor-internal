{
    "name": "rech-editor-internal",
    "displayName": "rech-editor-internal",
    "description": "Package for using Rech's extensions with Visual Studio Code in Rech's intranet.",
    "version": "0.0.1",
    "publisher": "Gustavo",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/RechInformatica/rech-editor-internal.git"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.openFontesTrunk",
                "title": "openFontesTrunk"
            },
            {
                "command": "extension.openScripts",
                "title": "openScripts"
            },
            {
                "command": "extension.openWc",
                "title": "openWc"
            }
        ],
        "keybindings": [
            {
                "command": "extension.openFontesTrunk",
                "key": "ctrl+shift+o"
            },
            {
                "command": "extension.openScripts",
                "key": "ctrl+shift+y"
            },
            {
                "command": "extension.openWc",
                "key": "alt+o"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "rech-editor-vscode": "file:../rech-editor-vscode"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
