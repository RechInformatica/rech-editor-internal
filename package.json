{
    "name": "rech-editor-internal",
    "displayName": "Rech Internal",
    "description": "Package for using Rech's extensions with Visual Studio Code in Rech's intranet",
    "version": "0.0.10",
    "publisher": "rechinformatica",
    "engines": {
        "vscode": "^1.35.0"
    },
    "icon": "images/rech.png",
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/RechInformatica/rech-editor-internal/issues",
        "email": "smcbreen@microsoft.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RechInformatica/rech-editor-internal.git"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "Log",
                "aliases": [
                    "Log Cobol"
                ],
                "extensions": [
                    ".log",
                    ".err"
                ],
                "configuration": "./log.configuration.json",
                "mimetypes": [
                    "log",
                    "text/log",
                    "text/x-log",
                    "text/x-code-output",
                    "x-code-output"
                ]
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Rech internal configurations",
            "properties": {
                "rech.editor.internal.autodiagnostic": {
                    "type": [
                        "boolean",
                        "string",
                        "string"
                    ],
                    "enum": [
                        false,
                        "onChange",
                        "onSave"
                    ],
                    "description": "Whether auto diagnostic is enabled",
                    "default": "onChange"
                },
                "rech.editor.internal.diagnosticfilter": {
                    "type": "array",
                    "description": "Filter of warnings that should not be considered in diagnostic"
                },
                "rech.editor.internal.lockReadOnlyFiles": {
                    "type": "boolean",
                    "description": "Whether the read-only files editing lock is enabled",
                    "default": "true"
                }
            }
        },
        "grammars": [
            {
                "language": "Log",
                "scopeName": "code.log",
                "path": "./syntaxes/log.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "rech.editor.internal.openFontesTrunk",
                "title": "Rech: Abrir no diretório do F:\\Fontes"
            },
            {
                "command": "rech.editor.internal.openCurrentSource",
                "title": "Rech: Abrir no diretório corrente sugerindo o nome do próprio fonte"
            },
            {
                "command": "rech.editor.internal.openScripts",
                "title": "Rech: Abrir no diretório do F:\\Bat"
            },
            {
                "command": "rech.editor.internal.openWc",
                "title": "Rech: Abrir no diretório do working-copy"
            },
            {
                "command": "rech.editor.internal.fonGrep",
                "title": "Rech: FonGrep"
            },
            {
                "command": "rech.editor.internal.openThis",
                "title": "Rech: Abre fonte da linha corrente"
            },
            {
                "command": "rech.editor.internal.RechWindowDesigner",
                "title": "Rech: Abre o RechWindowDesigner (RWD) do fonte atual"
            },
            {
                "command": "rech.editor.internal.OpenWFPF",
                "title": "Rech: Abre os copys WF/PF do fonte atual"
            },
            {
                "command": "rech.editor.internal.checkout",
                "title": "Rech: Checkout do fonte atual para o working copy"
            },
            {
                "command": "rech.editor.internal.checkoutdic",
                "title": "Rech: Checkoutdic - Atualiza dicionário do diretório por programador"
            },
            {
                "command": "rech.editor.internal.cleardic",
                "title": "Rech: Cleardic - Limpa dicionário do diretório por programador"
            },
            {
                "command": "rech.editor.internal.compile",
                "title": "Rech: Compilar fonte COBOL"
            },
            {
                "command": "rech.editor.internal.update",
                "title": "Rech: Update working-copy"
            },
            {
                "command": "rech.editor.internal.commit",
                "title": "Rech: Commit working-copy"
            },
            {
                "command": "rech.editor.internal.indent",
                "title": "Rech: Indenta mantendo nível atual"
            },
            {
                "command": "rech.editor.internal.indentLeft",
                "title": "Rech: Indenta para esquerda"
            },
            {
                "command": "rech.editor.internal.indentRight",
                "title": "Rech: Indenta para direita"
            },
            {
                "command": "rech.editor.internal.preprocess0",
                "title": "Rech: PREPROC - [0] Sem opções extras"
            },
            {
                "command": "rech.editor.internal.preprocess1",
                "title": "Rech: PREPROC - [1] - Mostrando hierarquia dos copys (-hc)"
            },
            {
                "command": "rech.editor.internal.preprocess2",
                "title": "Rech: PREPROC - [2] - Mostrando o uso de copys pelo programa (-lc)"
            },
            {
                "command": "rech.editor.internal.preprocess3",
                "title": "Rech: PREPROC - [3] - Mostrando os parágrafos (-lsp)"
            },
            {
                "command": "rech.editor.internal.preprocess4",
                "title": "Rech: PREPROC - [4] - Mostrando os parágrafos do programa (-lpp)"
            },
            {
                "command": "rech.editor.internal.preprocess5",
                "title": "Rech: PREPROC - [5] - Mostrando warnings normais e estendidos (-war -wes)"
            },
            {
                "command": "rech.editor.internal.preprocess6",
                "title": "Rech: PREPROC - [6] - Mostrando variáveis não usadas no programa (-vnp)"
            },
            {
                "command": "rech.editor.internal.preprocess7",
                "title": "Rech: PREPROC - [7] - Mostrando variáveis não usadas no programa e nos copys (-vnu)"
            },
            {
                "command": "rech.editor.internal.preprocess8",
                "title": "Rech: PREPROC - [8] - Mostrando warning de truncamento em MOVE  (-wop=w074)"
            },
            {
                "command": "rech.editor.internal.wm",
                "title": "Rech: WM - Compara o fonte atual com o trunk"
            },
            {
                "command": "rech.editor.internal.fcw_bet",
                "title": "Rech: FCW BET - Compara o fonte atual com a beta"
            },
            {
                "command": "rech.editor.internal.fcw_ofi",
                "title": "Rech: FCW OFI - Compara o fonte atual com a oficial"
            },
            {
                "command": "rech.editor.internal.fcw_est",
                "title": "Rech: FCW EST - Compara o fonte atual com a estendida"
            },
            {
                "command": "rech.editor.internal.fcw_versao",
                "title": "Rech: FCW - Compara o fonte atual com a versão informada pelo usuário"
            },
            {
                "command": "rech.editor.internal.fonlbrlog",
                "title": "Rech: FONLBRLOG - Visualiza alterações no SVN para fontes da versão Beta do SIGER"
            },
            {
                "command": "rech.editor.internal.fonteslog",
                "title": "Rech: FONTESLOG - Visualiza alterações no SVN para fontes da versão Desenvolvimento do SIGER"
            },
            {
                "command": "rech.editor.internal.lis",
                "title": "Rech: LIS - Lista as alterações no fonte"
            },
            {
                "command": "rech.editor.internal.lisbk",
                "title": "Rech: LISBK - Lista as alterações no fonte salvas em backup"
            },
            {
                "command": "rech.editor.internal.listbk",
                "title": "Rech: LISTBK - Ferramenta de comparação de fontes"
            },
            {
                "command": "rech.editor.internal.blame",
                "title": "Rech: BLAME - Visualiza alterações no SVN para fontes da versão Desenvolvimento do SIGER"
            },
            {
                "command": "rech.editor.internal.aplicaspd",
                "title": "Rech: APLICASPD - Executa ferramenta de aplicação de SPD"
            },
            {
                "command": "rech.editor.internal.autogrep",
                "title": "Rech: AUTOGREP - Executa ferramenta de busca de utilização de COPY"
            },
            {
                "command": "rech.editor.internal.pullComment",
                "title": "Rech: PUXA COMENTÞRIO - Puxa o comentário do elemento (parágrafo/variável) onde está o cursor"
            },
            {
                "command": "rech.editor.internal.findNextBlankLine",
                "title": "Rech Internal: Encontra próxima linha em branco"
            },
            {
                "command": "rech.editor.internal.findPreviousBlankLine",
                "title": "Rech Internal: Encontra linha em branco anterior"
            },
            {
                "command": "rech.editor.internal.convertToLowerCase",
                "title": "Rech Internal: Coverte trecho selecionado para COBOL minúsculo"
            },
            {
                "command": "rech.editor.internal.runTest",
                "title": "Rech Internal: Run Cobol unit test"
            },
            {
                "command": "rech.editor.internal.clearprog",
                "title": "Rech Internal: ClearProg - Remove programa do Working-copy"
            }
        ],
        "keybindings": [
            {
                "comment": "Ainda não é a rotina ideal... Mas por enquanto quebra um galho",
                "command": "editor.action.commentLine",
                "key": "alt+=",
                "when": "editorLangId == COBOL"
            },
            {
                "command": "rech.editor.internal.openFontesTrunk",
                "key": "ctrl+shift+o"
            },
            {
                "command": "rech.editor.internal.openScripts",
                "key": "ctrl+shift+y"
            },
            {
                "command": "rech.editor.internal.openWc",
                "key": "alt+o"
            },
            {
                "command": "rech.editor.internal.fonGrep",
                "key": "ctrl+l"
            },
            {
                "command": "rech.editor.internal.openThis",
                "key": "alt+f2"
            },
            {
                "command": "rech.editor.internal.checkout",
                "key": "alt+f5",
                "when": "editorLangId == COBOL && !textCompareEditorVisible"
            },
            {
                "command": "rech.editor.internal.compile",
                "key": "ctrl+f9",
                "when": "editorLangId == COBOL"
            },
            {
                "command": "rech.editor.internal.indent",
                "key": "ctrl+p",
                "when": "editorLangId == COBOL"
            },
            {
                "command": "rech.editor.internal.indentLeft",
                "key": "alt+shift+left",
                "when": "editorLangId == COBOL"
            },
            {
                "command": "rech.editor.internal.indentRight",
                "key": "alt+shift+right",
                "when": "editorLangId == COBOL"
            },
            {
                "command": "rech.editor.internal.pullComment",
                "key": "shift+alt+c",
                "when": "editorLangId == COBOL"
            },
            {
                "command": "rech.editor.internal.newLineAbove",
                "key": "ctrl+shift+enter"
            },
            {
                "command": "deleteWordRight",
                "key": "alt+w",
                "when": "textInputFocus && !editorReadonly"
            },
            {
                "command": "editor.action.nextMatchFindAction",
                "key": "F3"
            },
            {
                "command": "editor.action.previousMatchFindAction",
                "key": "F4"
            },
            {
                "command": "editor.action.previousMatchFindAction",
                "key": "shift+F3"
            },
            {
                "command": "deleteAllRight",
                "key": "alt+l"
            },
            {
                "command": "editor.action.deleteLines",
                "key": "ctrl+F12"
            },
            {
                "command": "rech.editor.internal.findWordForward",
                "key": "alt+0"
            },
            {
                "command": "rech.editor.internal.findWordBackward",
                "key": "alt+9"
            },
            {
                "command": "rech.editor.internal.copyWordUnderCursor",
                "key": "ctrl+abnt_c1"
            },
            {
                "command": "rech.editor.internal.replaceWordUnderCursor",
                "key": "shift+ctrl+abnt_c1"
            },
            {
                "key": "ctrl+right",
                "command": "cursorWordStartRight",
                "when": "editorTextFocus"
            },
            {
                "command": "rech.editor.internal.cobolUpdateLineDots",
                "key": "alt+abnt_c1"
            },
            {
                "command": "rech.editor.internal.findNextBlankLine",
                "key": "alt+p"
            },
            {
                "command": "rech.editor.internal.findPreviousBlankLine",
                "key": "alt+shift+p"
            },
            {
                "command": "rech.editor.internal.copyLine",
                "key": "F10",
                "when": "!inDebugMode"
            },
            {
                "command": "rech.editor.internal.pasteLine",
                "key": "F11",
                "when": "!inDebugMode"
            },
            {
                "command": "rech.editor.internal.replaceLine",
                "key": "shift+F11"
            },
            {
                "command": "editor.action.copyLinesUpAction",
                "key": "ctrl+shift+up"
            },
            {
                "command": "editor.action.copyLinesDownAction",
                "key": "ctrl+shift+down"
            },
            {
                "command": "rech.editor.internal.editorScrollUp",
                "key": "ctrl+up"
            },
            {
                "command": "rech.editor.internal.editorScrollDown",
                "key": "ctrl+down"
            },
            {
                "command": "editor.action.moveLinesDownAction",
                "key": "alt+shift+down"
            },
            {
                "command": "editor.action.moveLinesUpAction",
                "key": "alt+shift+up"
            },
            {
                "command": "editor.action.transformToLowercase",
                "key": "ctrl+alt+l"
            },
            {
                "command": "editor.action.transformToUppercase",
                "key": "ctrl+alt+u"
            },
            {
                "command": "rech.editor.internal.convertToLowerCase",
                "key": "shift+alt+l",
                "when": "editorLangId == COBOL"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "package": "vsce package --out F:/DIV/VSCode/extension/market/rech-editor-internal/rech-editor-internal.vsix",
        "compile": "npm run tslint && tsc -p ./",
        "watch": "tsc -watch -p ./",
        "preinstall": "npm install ../rech-editor-cobol/",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && mocha -r ts-node/register ./src/test/**/*.test.ts",
        "tslint": "tslint --project tsconfig.json --config tslint.json"
    },
    "dependencies": {
        "@types/chai": "^4.1.6",
        "@types/iconv-lite": "0.0.1",
        "@types/mocha": "^2.2.48",
        "@types/node": "^7.0.43",
        "chai": "^4.2.0",
        "iconv-lite": "^0.4.24",
        "mocha": "^5.2.0",
        "rech-editor-cobol": "file:../rech-editor-cobol",
        "ts-node": "^7.0.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vsce": "^1.53.0",
        "vscode": "^1.1.22"
    },
    "devDependencies": {}
}
